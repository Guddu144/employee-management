// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  role  Role
  phone String

}

enum Role {
  ADMIN
  EMPLOYER
  EMPLOYEE
}

model Employee {
  id    Int     @id @default(autoincrement())
  title String
  yearly_salary  Int
  address String 
}
